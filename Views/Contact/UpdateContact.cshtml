@model AcunMedyaHospitalProject.Entities.Contact
@{
    ViewBag.Title = "UpdateContact";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<div class="container">
    <form method="post">
        @Html.HiddenFor(x => x.Id)
        <label>Adres</label>
        @Html.TextBoxFor(x => x.Adress, new { @class = "form-control" })
        <br />
        <label>Telefon</label>
        @Html.TextBoxFor(x => x.Phone, new { @class = "form-control" })
        <br />
        <label>Email</label>
        @Html.TextBoxFor(x => x.Email, new { @class = "form-control" })
        <br />
        <label>Enlem</label>
        @Html.TextBoxFor(x => x.Latitude, new { @class = "form-control" })
        <br />
        <label>Boylam</label>
        @Html.TextBoxFor(x => x.Longitude, new { @class = "form-control" })
        <br />
        <button class="btn btn-info">Güncelle</button>
    </form>
</div>

